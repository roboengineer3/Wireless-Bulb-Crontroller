void setup() {
  // put your setup code here, to run once:
  Serial.begin(9600);

}

void loop() {
  // put your main code here, to run repeatedly:
 
 analogWrite(11, 255);
 delay(250);
   analogWrite(11, 200);
 delay(250);
  analogWrite(11, 100);
 delay(250);
  analogWrite(11, 50);
 delay(250);
   analogWrite(11, 25);
 delay(250);
   analogWrite(11, 0);
 delay(100);

}
